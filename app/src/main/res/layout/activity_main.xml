<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.q.neobjukbaseball.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Team 1"
        android:id="@+id/team1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Team 2"
        android:id="@+id/team2"
        android:layout_below="@+id/team1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_empty"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/team1"
        android:layout_alignEnd="@+id/team1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_1"
        android:layout_toRightOf="@id/team1"
        android:text="1"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_2"
        android:layout_toRightOf="@id/des_1"
        android:text="2"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_3"
        android:layout_toRightOf="@id/des_2"
        android:text="3"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_4"
        android:layout_toRightOf="@id/des_3"
        android:text="4"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_5"
        android:layout_toRightOf="@id/des_4"
        android:text="5"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_6"
        android:layout_toRightOf="@id/des_5"
        android:text="6"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_7"
        android:layout_toRightOf="@id/des_6"
        android:text="7"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_8"
        android:layout_toRightOf="@id/des_7"
        android:text="8"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_9"
        android:layout_toRightOf="@id/des_8"
        android:text="9"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_10"
        android:layout_toRightOf="@id/des_9"
        android:text="E"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_sum"
        android:layout_toRightOf="@id/des_10"
        android:text="0"
        android:layout_marginTop="16dp"
        android:layout_marginRight="7dp" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_11"
        android:layout_toRightOf="@id/team2"
        android:text="1"
        android:layout_marginTop="32dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_12"
        android:layout_toRightOf="@id/des_11"
        android:text="2"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_13"
        android:layout_toRightOf="@id/des_12"
        android:text="3"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_14"
        android:layout_toRightOf="@id/des_13"
        android:text="4"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_15"
        android:layout_toRightOf="@id/des_14"
        android:text="5"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_16"
        android:layout_toRightOf="@id/des_15"
        android:text="6"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_17"
        android:layout_toRightOf="@id/des_16"
        android:text="7"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_18"
        android:layout_toRightOf="@id/des_17"
        android:text="8"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_19"
        android:layout_toRightOf="@id/des_18"
        android:text="9"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_20"
        android:layout_toRightOf="@id/des_19"
        android:text="E"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/des_sum2"
        android:layout_toRightOf="@id/des_20"
        android:text="0"
        android:layout_marginTop="32dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="0"
        android:id="@+id/des_inn"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/des_sum"
        android:layout_toEndOf="@+id/des_sum"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="18dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="▲"
        android:id="@+id/des_td"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/des_inn"
        android:layout_toEndOf="@+id/des_inn"
        android:layout_marginTop="18dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Swing"
        android:id="@+id/btn_swing"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OUT"
        android:id="@+id/OUT"
        android:layout_below="@+id/team2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/team2"
        android:layout_alignEnd="@+id/team2"
        android:gravity="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/des_out"
        android:layout_alignTop="@+id/OUT"
        android:layout_toRightOf="@+id/OUT"
        android:layout_toEndOf="@+id/OUT"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="STRIKE"
        android:id="@+id/STRIKE"
        android:layout_below="@+id/OUT"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/des_out"
        android:layout_toStartOf="@+id/des_out"
        android:gravity="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/des_strike"
        android:layout_alignTop="@+id/STRIKE"
        android:layout_toRightOf="@+id/STRIKE"
        android:layout_toEndOf="@+id/STRIKE"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BALL"
        android:id="@+id/BALL"
        android:layout_below="@+id/STRIKE"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_toLeftOf="@+id/des_out"
        android:layout_toStartOf="@+id/des_out"
        android:gravity="center" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/des_ball"
        android:layout_alignTop="@+id/BALL"
        android:layout_toRightOf="@+id/BALL"
        android:layout_toEndOf="@+id/BALL"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="7dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="50dp"
        android:text="게임이 시작되었습니다."
        android:id="@+id/log"
        android:gravity="fill_horizontal|center_horizontal"
        android:layout_below="@+id/des_ball"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NOT Swing"
        android:id="@+id/btn_Notswing"
        android:layout_above="@+id/btn_swing"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
